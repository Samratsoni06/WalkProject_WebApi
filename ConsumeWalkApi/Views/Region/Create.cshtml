@model ConsumeWalkApi.Models.AddRegionModel

<h1>Add Region</h1>
<hr />

<form method="post">
   
    <div class="mt-3">
        <label class="form-label">Name</label>
        <input type="text" class="form-control" asp-for="Name" />
    </div>
    <div class="mt-3">
        <label class="form-label">Code</label>
        <input type="text" class="form-control" asp-for="Code" />
    </div>
    <div class="mt-3">
        <label class="form-label">RegionalImageUrl</label>
        <input type="text" class="form-control" asp-for="RegionalImageUrl" />
    </div>
    <div class="mt-3">
        <button type="submit" class="btn btn-primary">Submit</button>
    </div>

</form>